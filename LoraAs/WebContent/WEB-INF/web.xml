    <exception-type>java.lang.Throwable</exception-type>
    <location>/error/500.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404.jsp</location>
  </error-page> -->
  <display-name>LoraAs</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>NankaiServlet.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCust</servlet-name>
    <servlet-class>Nankai.web.AddCust</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListCustServlet</servlet-name>
    <servlet-class>Nankai.web.ListCustServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CusInfoServlet</servlet-name>
    <servlet-class>Nankai.web.CusInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updateCust</servlet-name>
    <servlet-class>Nankai.web.updateCust</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DelCustServlet</servlet-name>
    <servlet-class>Nankai.web.DelCustServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BatchDelServlet</servlet-name>
    <servlet-class>Nankai.web.BatchDelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Servlet/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCust</servlet-name>
    <url-pattern>/servlet/AddCust</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListCustServlet</servlet-name>
    <url-pattern>/ListCustServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CusInfoServlet</servlet-name>
    <url-pattern>/servlet/CusInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>updateCust</servlet-name>
    <url-pattern>/servlet/updateCust</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelCustServlet</servlet-name>
    <url-pattern>/servlet/DelCustServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BatchDelServlet</servlet-name>
    <url-pattern>/servlet/BatchDelServlet</url-pattern>
  </servlet-mapping>
</web-app>